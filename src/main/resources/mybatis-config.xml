<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>

	<typeAliases>
 		<typeAlias type="com.sesoc.csignup.vo.MemberVO" alias="MemberVO"/>  		
 		<typeAlias type="com.sesoc.csignup.vo.StudentVO" alias="StudentVO"/>  	
 		<typeAlias type="com.sesoc.csignup.vo.CourseVO" alias="CourseVO"/>  
 		<typeAlias type="com.sesoc.csignup.vo.EnrollVO" alias="EnrollVO"/>  
	</typeAliases>
	<mappers>
 		<mapper resource="mapper/memberMapper.xml" /> 
 		<mapper resource="mapper/studentMapper.xml" /> 
 		<mapper resource="mapper/courseMapper.xml" /> 
	</mappers>
</configuration>
